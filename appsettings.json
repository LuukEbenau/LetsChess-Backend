{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Warning",
      "Ocelot.": "Information"
    }
  },
  "Authentication": {
    "Google": {
      "ClientId": "528681707440-ge9ald71a4gdakd20vbrus84q1hjid52.apps.googleusercontent.com",
      "ClientSecret": "oTPjNSRP_CBP6kuaSvDDqzaj"
    }
  },
  "MQCredentials": {

  },
  "AllowedHosts": [
    "http://letschess.nl",
    "https://letschess.nl"
  ],
  "NLog": {
    "internalLogLevel": "Info",
    "internalLogFile": "c:\\temp\\internal-nlog.txt",
    "extensions": {
      "NLog.Web.AspNetCore": {
        "assembly": "NLog.Web.AspNetCore"
      }
    },
    "targets": {
      "ElasticSearch": {
        "type": "ElasticSearch",
        "index": "letschess-be-${date:format=yyyy.MM.dd}",
        "uri": "http://localhost:9200",
        "layout": "[${date:format=yyyy-MM-dd HH\\:mm\\:ss}][${level}] ${logger} ${message} ${exception:format=toString}",
        "fields": [
          {
            "Name": "time",
            "Layout": "${longdate}"
          },
          {
            "Name": "level",
            "Layout": "${level:uppercase=true}"
          },
          {
            "Name": "logger",
            "Layout": "${logger}"
          },
          {
            "Name": "message",
            "Layout": "${message}"
          },
          {
            "Name": "exception",
            "Layout": "${exception:format=toString}"
          },
          {
            "Name": "stacktrace",
            "Layout": "${stacktrace}"
          }
        ]
      }
    },
    "rules": [
      {
        "logger": "*",
        "minLevel": "Trace",
        "writeTo": "ElasticSearch"
      }
    ]
  }
}